---
interface Props {
  height?: string;
  width?: string;
}
const { height, width } = Astro.props;
---

<svg
  xmlns="http://www.w3.org/2000/svg"
  width={width}
  height={height}
  viewBox="0 -8 24 32"
  fill="none"
  stroke="currentColor"
  stroke-width="
  2"
  stroke-linecap="round"
  stroke-linejoin="round"
  class="text-[#3e3e3e]"
  fill="currentColor"
>
  <circle
    cx="8"
    cy="3.5"
    r=".5"
    class="animate-cauldron-bubble text-[#cde53b] opacity-0"
    fill="currentColor"
    style="animation-delay: 01s;"></circle>
  <circle
    cx="14"
    cy="6"
    r="2"
    class="animate-cauldron-bubble text-[#cde53b] opacity-0"
    fill="currentColor"
    style="animation-delay: 02s;"></circle>
  <path
    d="M19 2h.01"
    class="animate-cauldron-bubble text-[#cde53b] opacity-0"
    style="animation-delay: 03s;"></path>
  <path d="M22 8H2" class="" fill="currentColor"></path>
  <path d="M7 12V8" class="text-[#cde53b]" fill="currentColor"></path>
  <path d="M11 10V8" class="text-[#cde53b]" fill="currentColor"></path>
  <path
